<%= form_with model: @address do |f| %>
  <%= f.label :code %>
  <%= f.text_field :code %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.submit "登録" %>
<% end %>



<% @addresses.each do |address| %>
  <%= link_to edit_address_path(address) do %>
    <%= address.code %>
    <%= address.address %>
    <%= address.name %>
  <% end %>
<% end %>